@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/#> .
@prefix schema: <http://schema.org/> .
@prefix vf: <https://w3id.org/valueflows/> .


# Classes


vf:Agent
  a owl:Class ;
  rdfs:label "Agent" ;
  rdfs:comment "" ;
  owl:equivalentClass foaf:Agent .

vf:Person
  a owl:Class ;
  rdfs:label "Person" ;
  rdfs:comment "" ;
  rdfs:subClassOf vf:Agent ;
  owl:equivalentClass foaf:Person, schema:Person .

vf:Group
  a owl:Class ;
  rdfs:label "Group" ;
  rdfs:comment "" ;
  rdfs:subClassOf vf:Agent ;
  owl:equivalentClass foaf:Group .

vf:Relationship
  a owl:Class ;
  rdfs:label "Relationship" ;
  rdfs:comment "" .
  
vf:RelationshipType
  a owl:Class ;
  rdfs:label "Relationship Type" ;
  rdfs:comment "" ;
  rdfs:subClassOf rdf:Property .


# Properties

vf:label
  a owl:DatatypeProperty ;
  rdfs:label "label" ;
  rdfs:range rdfs:Literal ;
  rdfs:domain vf:Agent, vf:Relationship  .

vf:url
  a owl:DatatypeProperty ;
  rdfs:label "url" ;
  rdfs:domain vf:Agent .

vf:image
  a owl:ObjectProperty ;
  rdfs:label "image" ;
  rdfs:domain vf:Agent .  

vf:subject
  a owl:ObjectProperty ;
  rdfs:label "subject" ;
  rdfs:domain vf:Relationship;
  rdfs:range vf:Agent .

vf:relationship
  a vf:RelationshipType ;
  rdfs:label "relationship" ;
  rdfs:domain vf:Relationship ;
  rdfs:range vf:RelationshipType .

vf:object
  a owl:ObjectProperty ;
  rdfs:label "object" ;
  rdfs:domain vf:Relationship;
  rdfs:range vf:Agent .

vf:context
  a owl:ObjectProperty ;
  rdfs:label "context" ;
  rdfs:domain vf:Relationship ;
  rdfs:range vf:Agent .
  
vf:note
  a owl:DatatypeProperty ;
  rdfs:label "note" ;
  rdfs:domain vf:Agent, vf:Relationship .

vf:memberOf
  a vf:RelationshipType ;
  skos:prefLabel "is member of" ;
  owl:inverseOf vf:hasMember ;
  rdfs:range vf:RelationshipType.

vf:hasMember
  a vf:RelationshipType ;
  skos:prefLabel "has member" ;
  owl:inverseOf vf:memberOf ;
  rdfs:range vf:RelationshipType .

